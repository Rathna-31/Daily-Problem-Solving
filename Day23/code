class Solution(object):
    def titleToNumber(self, columnTitle):
        """
        :type columnTitle: str
        :rtype: int
        """
        
        r = 0

        for i in range(len(columnTitle)):
            r = r * 26 + (ord(columnTitle[i]) - 64)
        
        return r
        

        
        

        



        


        
